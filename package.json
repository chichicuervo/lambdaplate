{
  "name": "boilerplate-lambda",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "concurrently \"npm run start:dev:client\" \"npm run start:dev:server\"",
    "start:dev:client": "webpack-dev-server --config config/client.webpack.js",
    "start:dev:server": "cross-env NODE_ENV=offline serverless offline",
    "build:client": "cross-env NODE_ENV=production webpack --config config/client.webpack.js",
    "deploy": "cross-env AWS_REGION=us-west-2 serverless deploy -v --aws-profile redtorch -s dev -r us-west-2",
    "remove": "cross-env AWS_REGION=us-west-2 serverless remove -v --aws-profile redtorch -s dev -r us-west-2"
  },
